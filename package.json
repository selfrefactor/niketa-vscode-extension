{
	"name": "niketa-vscode-extension",
	"displayName": "Run Niketa scripts",
	"description": "Request to run scripts defined in `package.json`",
	"version": "0.1.0",
	"license": "MIT",
	"publisher": "selfrefactor",
	"icon": "logo.png",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"main": "./src/extension",
	"files": [
		"src"
	],
	"activationEvents": [
		"onCommand:niketa.request.run.first",
		"onCommand:niketa.request.run.second"
	],
	"contributes": {
		"commands": [
			{
				"command": "niketa.request.run.first",
				"title": "Niketa - run with current file | index 0"
			},
			{
				"command": "niketa.request.run.second",
				"title": "Niketa - run with current file | index 1"
			}
		],
		"keybindings": [
			{
				"command": "niketa.request.run.first",
				"key": "ctrl+1"
			},
			{
				"command": "niketa.request.run.second",
				"key": "alt+w"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "jest src --watch"
	},
	"devDependencies": {
		"vscode": "1.1.37"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/selfrefactor/niketa-vscode-extension.git"
	},
	"dependencies": {
		"minimatch": "9.0.3",
		"rambdax": "9.0.0"
	}
}
